{
  "name": "dravencms/admin",
  "type": "dravencms-package",
  "description": "Admin module for dravencms",
  "keywords": [
    "dravencms",
    "admin"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/user": "@dev",
    "ipub/gravatar": "^2.0"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "timeline": "Dravencms\\Admin\\DI\\AdminExtension"
        }
      },
      "scripts": {
        "post-install": "Dravencms\\Admin\\Script\\PostInstall",
        "pre-uninstall": "Dravencms\\Admin\\Script\\PreUninstall"
      }
    }
  }
}