{
  "name": "dravencms/admin",
  "type": "dravencms-package",
  "description": "Admin module for dravencms",
  "keywords": [
    "dravencms",
    "admin"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/user": "^v1.0",
    "ipub/gravatar": "v2.2.0|v1.0.1|v2.2.2"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "files": {
        "assets/*": "%wwwDir%/assets/admin"
      },
      "configuration": "dravencms.config.neon"
    }
  }
}
